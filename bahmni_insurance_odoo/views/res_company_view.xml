<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_company_inherit_form_view" model="ir.ui.view">
        <field name="name">Res Company Inherit form View </field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"></field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='favicon']" position="after">                
                <field name="hospital_type"></field>
                <field name="copayment"></field>
            </xpath>
        </field>
    </record>
</odoo>